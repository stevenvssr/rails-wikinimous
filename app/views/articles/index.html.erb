<h1>Articles</h1>


<button type="button" class="btn btn-dark btn-lg"><%= link_to "Add a new article", "http://localhost:3000/articles/new" %></button>

<% @articles.each do |article| %>
<div class="card">
  <div class="card-body">
    <%= link_to article.title, article_path(article) %>
  </div>
</div>
<% end %>
